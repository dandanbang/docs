{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Life Note Library",
  "colors": {
    "primary": "#e5d1a9",
    "light": "#3d1b89",
    "dark": "#e5d1a9"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "icon": "book-open-cover",
          "href": "https://mintlify.com/docs"
        },
        {
          "anchor": "Blog",
          "icon": "newspaper",
          "href": "https://blog.mylifenote.ai/"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Life Note Guides",
        "groups": [
          {
            "group": "Start Here",
            "icon": "arrow-down-to-line",
            "expanded": true,
            "pages": [
              "life-note-baisc"
            ]
          },
          {
            "group": "Life Note Essential",
            "icon": "book-open",
            "pages": [
              "essentials/settings",
              "essentials/navigation",
              "essentials/untitled-page",
              "essentials/wisdom-collection"
            ]
          },
          {
            "group": "Account",
            "icon": "box-heart",
            "pages": [
              "gift-card",
              "essentials/code"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "hidden": true,
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/images/TransparentLifeNoteLogo.png",
    "dark": "/images/LifeNoteLogoTransparentWhite.png",
    "href": "https://www.mylifenote.ai/"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/lifenote_ai",
      "linkedin": "https://www.linkedin.com/company/mylifenote",
      "instagram": "https://www.instagram.com/lifenote.journal",
      "discord": "https://discord.gg/JQ7HqyMQ9b",
      "threads": "https://www.threads.net/@mylifenote.ai",
      "website": "https://www.mylifenote.ai/"
    }
  },
  "appearance": {
    "default": "dark"
  },
  "fonts": {
    "family": "Open Sans"
  }
}
